<div class='card'>
  <div class='card-body'>
    <div class='row'>
      <div class='col-md-12'>
        <%= form_tag csv_upload_contacts_path, multipart: true do %>
          <% label_tag :file %>
          <%= file_field_tag :file %> 
          <br>
          <br>
          <%= submit_tag "Import Contact CSV", class: 'btn btn-success' %>
        <% end %>
      </div>
    </div>
  </div>
</div>